{
  "editor.rulers": [
    80
  ],
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingDelete": "never",
  "editor.autoClosingOvertype": "never",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "smooth",
  "editor.foldingImportsByDefault": true,
  "editor.fontFamily": "'Iosevka Nerd Font','Droid Sans Mono', 'monospace', monospace",
  "editor.fontSize": 14,
  "editor.glyphMargin": false,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "trailing",
  "editor.showFoldingControls": "never",
  "editor.stickyScroll.enabled": true,
  "editor.tabSize": 2,
  "extensions.autoCheckUpdates": false,
  "extensions.autoUpdate": false,
  "git.autofetch": true,
  "haskell.manageHLS": "PATH",
  "metals.testUserInterface": "Code Lenses",
  "nix.enableLanguageServer": true,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.shellIntegration.decorationsEnabled": "never",
  "terminal.integrated.smoothScrolling": true,
  "update.mode": "none",
  "window.menuBarVisibility": "toggle",
  "workbench.colorTheme": "Nord",
  "workbench.editor.showTabs": false,
  "workbench.preferredLightColorTheme": "Default Dark+",
  "workbench.startupEditor": "none",
  "workbench.colorCustomizations": {
    "editorWhitespace.foreground": "#ff0000"
  },
  "multiCommand.commands": [
    {
      "command": "multiCommand.toggleAllPanels",
      "sequence": [
        "workbench.action.togglePanel",
        "workbench.action.toggleSidebarVisibility",
        "workbench.action.focusActiveEditorGroup"
      ]
    }
  ],
  "vim.ignorecase": false,
  "vim.foldfix": true,
  "vim.leader": ",",
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "w"],
      "commands": [
        "editor.action.trimTrailingWhitespace"
      ]
    },
    {
      "before": ["<leader>", "z"],
      "commands": [
        "git.revertSelectedRanges"
      ]
    },
    {
      "before": ["<leader>", "f"],
      "commands": [
        "workbench.files.action.showActiveFileInExplorer"
      ]
    },
    {
      "before": ["[", "h"],
      "commands": [
        "workbench.action.editor.previousChange"
      ]
    },
    {
      "before": ["]", "h"],
      "commands": [
        "workbench.action.editor.nextChange"
      ]
    },
    {
      "before": ["]", "w"],
      "commands": [
        "editor.action.marker.prev"
      ]
    },
    {
      "before": ["]", "w"],
      "commands": [
        "editor.action.marker.next"
      ]
    },
    {
      "before": ["[", "b"],
      "commands": [
        "workbench.action.navigateBack"
      ]
    },
    {
      "before": ["]", "b"],
      "commands": [
        "workbench.action.navigateForward"
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["alt", "c"],
      "commands": [
        "editor.action.clipboardCopyAction"
      ]
    }
  ],
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "task.problemMatchers.neverPrompt": {
    "shell": true
  },
  "metals.serverVersion": "0.11.11+3-81dab9c6-SNAPSHOT",
  "metals.showImplicitArguments": false,
  "metals.showInferredType": false
}
